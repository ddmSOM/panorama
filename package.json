{
  "name": "@stamen/panorama",
  "version": "0.1.8",
  "description": "Panorama visualization toolkit",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev-examples",
    "test": "jest",
    "lint": "eslint ./src",
    "postinstall": "npm run build-modules",
    "build-modules": "gulp build",
    "dev-modules": "gulp dev",
    "publish-modules": "./_build-config/publish-modules.sh",
    "build-examples": "npm run clean-examples && webpack --config ./_examples/webpack.config.ghpage.js",
    "dev-examples": "npm run build-modules; npm run clean-examples && webpack-dev-server --config ./_examples/webpack.config.js --hot --inline --content-base _examples/public",
    "publish-examples": "./_build-config/publish-examples.sh",
    "clean-examples": "rm -rf ./_examples/public/examples.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stamen/panorama.git"
  },
  "keywords": [
    "visualization",
    "geospatial",
    "maps",
    "mapping",
    "dataviz",
    "geoviz"
  ],
  "author": "stamen",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "scriptPreprocessor": "<rootDir>/_test/lib/preprocessor.js",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "build"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/fbjs"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "git-directory-deploy": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "topojson": "^1.6.24",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "@stamen/areachart": "file:areachart",
    "@stamen/cartodb-loader": "file:cartodb-loader",
    "@stamen/cartodb-tilelayer": "file:cartodb-tilelayer",
    "@stamen/chartslider": "file:chartslider",
    "@stamen/choropleth": "file:choropleth",
    "@stamen/discretebarchart": "file:discretebarchart",
    "@stamen/hashmanager": "file:hashmanager",
    "@stamen/horizontaldiscretebarchart": "file:horizontaldiscretebarchart",
    "@stamen/intromanager": "file:intromanager",
    "@stamen/itemselector": "file:itemselector",
    "@stamen/leaflet-choropleth": "file:leaflet-choropleth",
    "@stamen/leaflet-donut": "file:leaflet-donut",
    "@stamen/leaflet-tooltip": "file:leaflet-tooltip",
    "@stamen/legend": "file:legend",
    "@stamen/linechart": "file:linechart",
    "@stamen/navigation": "file:navigation",
    "@stamen/offsetareachart": "file:offsetareachart",
    "@stamen/punchcard": "file:punchcard",
    "@stamen/scatterplot": "file:scatterplot",
    "@stamen/textlist": "file:textlist",
    "@stamen/timebasedmarkers": "file:timebasedmarkers"
  }
}
